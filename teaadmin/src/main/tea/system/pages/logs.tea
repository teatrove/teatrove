<% template logs (Map config)

    admin = getTeaServletAdmin();
    request = getRequest();

    call system.util.handleRequest(request);

%>
		<article>
			<section>
				<div class="controls" style="width: 100%;">
					<div class="controls-wrapper left-float">
                        <div><input id="logs-search-text" type="search" placeholder="Search" /></div>
                        <div id="showHideLogs" class="button">Show All</div>
					</div>
				</div>
				<br class="clear" />
				<div id="logs-container"></div>
			</section>
		</article>