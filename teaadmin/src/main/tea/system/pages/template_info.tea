                                <td class="right-align"><% if ( rawDatum.endTime == -1 ) { '--' } else { itemEnd } %></td>

                                <% numberFormat("#,###.00"); %>
                                <td class="right-align"><% rawDatum.duration %></td>

                                <% numberFormat("#,###.00"); %>
                                <td class="right-align"><% rawDatum.contentLength/1024.0 %></td>
