                                <tr class="colhead">
                                    <th class="right-align">Average</th>
                                    <th class="right-align">Min</th>
                                    <th class="right-align">Max</th>
                                    <th class="right-align">StdDev</th>

                                    <th class="right-align">Average</th>
                                    <th class="right-align">Min</th>
                                    <th class="right-align">Max</th>
                                    <th class="right-align">StdDev</th>
