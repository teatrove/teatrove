<%
                                if ( templateStats != null and aggregateIntervals != null ) {
                                    foreach ( i in 0..aggregateIntervals.length-1 ) {
                                        item = aggregateIntervals[i];

                                        rowClass = 'oddrow';
                                        if ( i % 2 == 1 ) {
                                            rowClass = 'evenrow';
                                        }

